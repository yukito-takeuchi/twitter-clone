{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "node dist/scripts/migrate.js",
    "heroku-postbuild": "npm run build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f backend",
    "docker:db:logs": "docker-compose logs -f postgres",
    "docker:restart": "docker-compose restart backend",
    "docker:rebuild": "docker-compose up -d --build",
    "docker:migrate": "docker-compose exec backend npm run migrate"
  },
